{
  "name": "vite-catalog",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.19.1"
  },
  "scripts": {
    "postbuild": "npm run move-files",
    "postbuild-prod": "npm run move-files",
    "dev": "vite dev",
    "move-files": "find dist -mindepth 1 -not -path 'dist/analog*' -exec rm -rf {} + && shx mv dist/analog/public/* dist && shx rm -rf dist/analog",
    "build": "ng build --configuration development",
    "build-prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "preview": "vite preview"
  },
  "private": true,
  "dependencies": {
    "@analogjs/content": "^1.10.3",
    "@analogjs/router": "^1.10.3",
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/elements": "^19.0.5",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@dp-wk/emitter": "file:../../packages/emitter",
    "@dp-wk/store": "file:../../packages/store",
    "@module-federation/vite": "^1.1.3",
    "@rollup/wasm-node": "^4.29.1",
    "esbuild-wasm": "^0.24.2",
    "front-matter": "^4.0.2",
    "marked": "^5.0.2",
    "marked-gfm-heading-id": "^3.1.0",
    "marked-highlight": "^2.0.1",
    "marked-mangle": "^1.1.7",
    "prismjs": "^1.29.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "vite-plugin-node-polyfills": "^0.22.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.10.3",
    "@analogjs/vite-plugin-angular": "^1.10.3",
    "@analogjs/vitest-angular": "^1.10.3",
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/build": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@rollup/plugin-wasm": "^6.2.2",
    "autoprefixer": "^10.4.14",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.21",
    "shx": "^0.3.4",
    "tailwindcss": "^3.3.1",
    "typescript": "~5.5.0",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^2.0.0",
    "wait-on": "^8.0.1"
  },
  "overrides": {
    "rollup": "npm:@rollup/wasm-node",
    "esbuild": "npm:esbuild-wasm"
  }
}
